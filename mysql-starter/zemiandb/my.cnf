# Default Homebrew MySQL server config
[mysqld]

# Only allow connections from localhost
bind-address = 127.0.0.1

# Do not restrict LOAD DATA and allow read any directory for easy dev
secure-file-priv=

# Set default server timezone so JDBC driver can connect
# This setting must requires server restart!
#default-time-zone='SYSTEM'
#default-time-zone='-5:00'
default-time-zone='+0:00'
